all: ${OBJ}
	${CC} -o ${NAME} ${OBJ}

.PHONY: clean oclean fclean re
clean:
	${RM} *~ ${NAME}
oclean:
	${RM} *~ ${OBJ}
fclean:
	${RM} *~ ${NAME}
	${RM} *~ ${OBJ}
re: oclean all
